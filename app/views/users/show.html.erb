<div class="container">
  <div class="boxes">
    <div class="box">
      <h1><%= current_user.nickname %></h1>
      <%= link_to "プロフィール編集", edit_user_registration_path, class: "box-text" %>
    </div>
    
    <% current_user.questions.order(created_at: :desc).each do |question| %>
      <div class="box">
        <%= link_to(question.text, "/questions/#{question.id}", class: "box-text")  %>
      </div>
    <% end %>
  </div>
</div>
